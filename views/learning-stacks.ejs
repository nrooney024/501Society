<html>
	<head>
		<title>My Learning Stacks</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<link rel="icon" href="/images/501-society-tab-icon-round.png">
		<link rel="stylesheet" href="/css/main.css" />
		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link href="https://fonts.googleapis.com/css2?family=Exo:wght@100&family=Roboto&display=swap" rel="stylesheet">
	</head>
	<body class="no-sidebar is-preload">
		<div id="page-wrapper">


		

			<!-- Header -->
				<section id="header" class="wrapper">

					<!-- Logo -->
						<div id="logo">
							<h1><a href="/">My Learning Stacks</a></h1>
						</div>

					<!-- Nav -->
						<nav id="nav">
							<ul>
								<li class="current"><a href="/">Home</a></li>
								<li><a href="learning-stacks">Learning Stacks</a></li>
								<li><a href="/logout" class="logout__btn">Logout</a></li>
							</ul>
						</nav>

						<button type="button" class="add-learning-stack-button" onclick="openForm()" >Add Learning Stack +</button>
					
				</section>

			
				<!-- Add learning stack popup -->
			
				<form action="/learning-stacks/createLearningStack" method='POST' class="add-learning-stack-form" id="addLearningStackForm">
					<button class="close-form" onclick="closeForm()">Close</button>
					<br>
					<h2 class="add-learning-stack-header">Add a Learning Stack</h2>
					<label for="learning-stack-name" class="form-name">Learning Stack Name</label>
					<input type="text" placeholder="Enter the Name of Your Learning Stack" name="learningStackName">
					<input type="submit" value="Submit" class="submit-form">
				</form>

				



			<!-- Main -->
				<div id="main" class="wrapper style2">
					<div class="container">
						<!-- Content -->
							<div id="content">

								<!-- Filters -->
									<nav class="filters">
										<a href="#">Tiles</a>
										<a href="#">List</a>
										<a href="#">Sort</a>
										<a href="#">Tags</a>
									</nav>	

								<!-- Tiles -->
									<!-- <a href="/learning-stack-page">
										<div class="tiles">
											<img src="/images/test-tile-img.png" alt="Khan Academy">
											<h3>Python</h3>
											<span>Last Opened: June 19th, 2022</span>
										</div>
									</a> -->

									<div class="tilesContainer">
										
										<% for(var i=0; i<learningStacks.length; i++) {%>						
											  <!-- <a href="/post/<%= posts[i]._id%>"> -->
												<div class="tile">
													<h3><%= learningStacks[i].learningStackName %></h3>
													<span><%= learningStacks[i].dateCreated %></span>
												</div>
											  <!-- </a> -->
										  <% } %>

									</div>
							</div>
					</div>
				</div>

			<!-- Footer -->
				<section id="footer" class="wrapper">
					<div class="container">
						<header class="style1">
							<h2>Ipsum sapien elementum portitor?</h2>
							<p>
								Sed turpis tortor, tincidunt sed ornare in metus porttitor mollis nunc in aliquet.<br />
								Nam pharetra laoreet imperdiet volutpat etiam feugiat.
							</p>
						</header>
						<div class="row">
							<div class="col-6 col-12-medium">

								<!-- Contact Form -->
									<section>
										<form method="post" action="#">
											<div class="row gtr-50">
												<div class="col-6 col-12-small">
													<input type="text" name="name" id="contact-name" placeholder="Name" />
												</div>
												<div class="col-6 col-12-small">
													<input type="text" name="email" id="contact-email" placeholder="Email" />
												</div>
												<div class="col-12">
													<textarea name="message" id="contact-message" placeholder="Message" rows="4"></textarea>
												</div>
												<div class="col-12">
													<ul class="actions">
														<li><input type="submit" class="style1" value="Send" /></li>
														<li><input type="reset" class="style2" value="Reset" /></li>
													</ul>
												</div>
											</div>
										</form>
									</section>

							</div>
							<div class="col-6 col-12-medium">

								<!-- Contact -->
									<section class="feature-list small">
										<div class="row">
											<div class="col-6 col-12-small">
												<section>
													<h3 class="icon solid fa-home">Mailing Address</h3>
													<p>
														Untitled Corp<br />
														1234 Somewhere Rd<br />
														Nashville, TN 00000
													</p>
												</section>
											</div>
											<div class="col-6 col-12-small">
												<section>
													<h3 class="icon solid fa-comment">Social</h3>
													<p>
														<a href="#">@untitled-corp</a><br />
														<a href="#">linkedin.com/untitled</a><br />
														<a href="#">facebook.com/untitled</a>
													</p>
												</section>
											</div>
											<div class="col-6 col-12-small">
												<section>
													<h3 class="icon solid fa-envelope">Email</h3>
													<p>
														<a href="#">info@untitled.tld</a>
													</p>
												</section>
											</div>
											<div class="col-6 col-12-small">
												<section>
													<h3 class="icon solid fa-phone">Phone</h3>
													<p>
														(000) 555-0000
													</p>
												</section>
											</div>
										</div>
									</section>

							</div>
						</div>
						<div id="copyright">
							<ul>
								<li>&copy; Untitled.</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
							</ul>
						</div>
					</div>
				</section>

		</div>

		<!-- Scripts -->
			<script src="/js/jquery.min.js"></script>
			<script src="/js/jquery.dropotron.min.js"></script>
			<script src="/js/browser.min.js"></script>
			<script src="/js/breakpoints.min.js"></script>
			<script src="/js/util.js"></script>
			<script src="/js/main.js"></script>

	</body>
</html>